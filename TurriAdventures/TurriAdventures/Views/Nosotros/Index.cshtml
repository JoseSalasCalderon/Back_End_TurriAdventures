@model IEnumerable<Entities.Entities.Nosotros>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionNosotros)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagenNosotros)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionNosotros)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagenNosotros)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNosotros">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNosotros">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNosotros">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
